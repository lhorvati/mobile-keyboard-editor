<?xml version='1.0' encoding='utf-8'?>
<widget id="com.editor.beta" version="0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Editor</name>
    <author href="">
        Lkraljevic
    </author>
    <content src="index.html" />
    <icon src="icon-3.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="23" />
    <preference name="AndroidLaunchMode" value="standard" />
    <preference name="BackgroundColor" value="#222" />
    <preference name="Orientation" value="portrait" />
    <preference name="SplashScreen" value="screen" />
</widget>
